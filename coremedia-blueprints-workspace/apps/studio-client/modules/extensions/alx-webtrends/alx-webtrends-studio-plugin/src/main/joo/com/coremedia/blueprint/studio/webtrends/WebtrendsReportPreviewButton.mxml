<?xml version="1.0" encoding="UTF-8"?>
<!---
 Button to open a new browser window with the Webtrends report for the current preview document.
-->
<alx:OpenAnalyticsDeepLinkUrlButton
        xmlns:fx="http://ns.adobe.com/mxml/2009"
        xmlns:exml="http://www.jangaroo.net/exml/0.8"
        xmlns="exml:ext.config"
        xmlns:alx="exml:com.coremedia.blueprint.studio.config.analytics"
        windowName="WebtrendsReportWindow"
        tooltip="{resourceManager.getString('com.coremedia.blueprint.studio.webtrends.WebtrendsStudioPlugin', 'webtrends_preview_btn_tooltip')}"
        text="{resourceManager.getString('com.coremedia.blueprint.studio.webtrends.WebtrendsStudioPlugin', 'webtrends_preview_btn_tooltip')}"
        itemId="btn-analytics-report-webtrends"
        iconCls="{resourceManager.getString('com.coremedia.icons.CoreIcons', 'analytics')}"
        ui="{ButtonSkin.WORKAREA.getSkin()}"
        scale="medium"
        serviceName="webtrends">
  <fx:Metadata>
    [ResourceBundle('com.coremedia.blueprint.studio.webtrends.WebtrendsStudioPlugin')]
    [ResourceBundle('com.coremedia.icons.CoreIcons')]
  </fx:Metadata>
  <fx:Script><![CDATA[
    import com.coremedia.ui.skins.ButtonSkin;

    public static const xtype:String = "com.coremedia.blueprint.studio.config.webtrends.webtrendsReportPreviewButton";

    private var config:WebtrendsReportPreviewButton;

    public native function WebtrendsReportPreviewButton(config:WebtrendsReportPreviewButton = null);
    ]]></fx:Script>

</alx:OpenAnalyticsDeepLinkUrlButton>
