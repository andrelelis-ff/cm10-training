<?xml version="1.0" encoding="UTF-8"?>
<livecontext:CommerceWorkAreaTab
        xmlns:fx="http://ns.adobe.com/mxml/2009"
        xmlns:exml="http://www.jangaroo.net/exml/0.8"
        xmlns="exml:ext.config"
        xmlns:livecontext="exml:com.coremedia.livecontext.studio.config">
  <fx:Script><![CDATA[
    public static const xtype:String = "com.coremedia.livecontext.studio.config.commerceProductWorkAreaTab";

    public static const CONTENT_TAB_ITEM_ID:String = "contentTab";

    public static const STRUCTURE_TAB_ITEM_ID:String = "structureTab";

    public static const SYSTEM_TAB_ITEM_ID:String = "systemTab";

    private var config:CommerceProductWorkAreaTab;

    public native function CommerceProductWorkAreaTab(config:CommerceProductWorkAreaTab = null);
    ]]></fx:Script>

  <livecontext:subTabs>
    <livecontext:CommerceProductContentForm itemId="{CONTENT_TAB_ITEM_ID}"/>
    <livecontext:CommerceAttributesForm/>
    <livecontext:CommerceProductStructureForm itemId="{STRUCTURE_TAB_ITEM_ID}"/>
    <livecontext:CommerceSystemForm autoHide="true" itemId="{SYSTEM_TAB_ITEM_ID}"/>
  </livecontext:subTabs>

</livecontext:CommerceWorkAreaTab>
