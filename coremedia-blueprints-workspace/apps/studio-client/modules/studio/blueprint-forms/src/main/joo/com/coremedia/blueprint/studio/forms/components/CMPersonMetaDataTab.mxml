<?xml version="1.0" encoding="UTF-8"?>
<editor:DocumentForm
        xmlns:fx="http://ns.adobe.com/mxml/2009"
        xmlns:exml="http://www.jangaroo.net/exml/0.8"
        xmlns="exml:ext.config"
        xmlns:editor="exml:com.coremedia.cms.editor.sdk.config"
        xmlns:bpforms="exml:com.coremedia.blueprint.studio.config"
        title="{resourceManager.getString('com.coremedia.blueprint.studio.BlueprintTabs', 'Tab_extras_title')}"
        itemId="metadata">
  <fx:Metadata>
    [ResourceBundle('com.coremedia.blueprint.studio.BlueprintTabs')]
  </fx:Metadata>
  <fx:Script><![CDATA[
    public static const xtype:String = "com.coremedia.blueprint.studio.config.personMetadataTab";

    private var config:CMPersonMetaDataTab;

    public native function CMPersonMetaDataTab(config:CMPersonMetaDataTab = null);
    ]]></fx:Script>

  <editor:items>
    <bpforms:PersonSEOForm bindTo="{config.bindTo}"
                           delegatePropertyNames="{['firstName','lastName','displayName']}"/>
  </editor:items>

</editor:DocumentForm>
