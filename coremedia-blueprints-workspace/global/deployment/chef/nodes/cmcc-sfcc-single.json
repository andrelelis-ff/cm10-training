{
  "blueprint": {
    "hostname": "CMS_HOST",
    "loopback_hostname": "localhost",
    "maven_repository_url": "file://localhost/tmp/maven-repo",
    "apps": {
      "content-management-server": {
        "application.properties": {
          "cap.server.license": "properties/corem/license.zip"
        }
      },
      "master-live-server": {
        "application.properties": {
          "cap.server.license": "properties/corem/license.zip"
        }
      },
      "commerce-adapter-sfcc": {
        "application.properties": {
          "sfcc.ocapi.host": "SFCC_HOST",
          "sfcc.ocapi.sandbox": "true",
          "sfcc.oauth.clientId": "SFCC_CLIENT_ID",
          "sfcc.oauth.clientPassword": "SFCC_CLIENT_PASSWORD",
          "sfcc.link.storefront-host": "SFCC_LINK_STOREFRONT_HOST",
          "link.storefront-url": "LINK_STOREFRONT_URL"
        }
      }
    },
    "sfcc": {
      "enabled": true,
      "host": "SFCC_HOST"
    },
    "dev": {
      "content": {
        "content_zip": "file://localhost/tmp/content-users.zip"
      }
    }
  },
  "run_list": [
    "role[mysql]",
    "role[mongodb]",
    "role[solr]",
    "role[commerce-adapter]",
    "role[cmcc-config]",
    "role[management]",
    "role[publication]",
    "role[preview]",
    "role[preview-headless]",
    "role[studio]",
    "role[cmcc-studio-proxy]",
    "role[delivery]",
    "role[delivery-headless]",
    "role[cmcc-delivery-proxy]",
    "recipe[blueprint-cmcc::overview]",
    "recipe[blueprint-dev-tooling::content]"
  ]
}
